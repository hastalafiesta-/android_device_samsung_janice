ro.device.cpu=ARMv7 1GHz Dual-core ARM Cortex-A9
ro.device.gpu=Mali-400MP Single Core
ro.device.rear_cam=5 Mpix (2592x1944)
ro.device.front_cam=1.3 Mpix (1280x1024)
